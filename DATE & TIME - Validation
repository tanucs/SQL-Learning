--------VALIDATION-------

/*ISDATE()
*/

-- Example --
SELECT
    OrderDate,
    ISDATE(OrderDate) AS IsValidDate,
    CASE 
        WHEN ISDATE(OrderDate) = 1 
        THEN CAST(OrderDate AS DATE)
        ELSE CAST('9999-01-01' AS DATE)
    END AS NewOrderDate
FROM 
(
    SELECT '2025-08-20' AS OrderDate UNION
    SELECT '2025-08-21' UNION
    SELECT '2025-08023'
) AS t;
